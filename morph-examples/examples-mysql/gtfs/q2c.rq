#Q2C: Check the number of routes of a particular agency
#AGENCYID = <http://linkedgtfs.com/CITY/agency/CRTM>

PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX foaf: <http://xmlns.com/foaf/0.1/>
PREFIX gtfs: <http://vocab.gtfs.org/terms#>
PREFIX geo: <http://www.w3.org/2003/01/geo/wgs84_pos#>
PREFIX dct: <http://purl.org/dc/terms/>

SELECT (count(distinct ?route) as ?count) WHERE {
	<http://linkedgtfs.com/CITY/agency/CRTM> a gtfs:Agency .
	<http://linkedgtfs.com/CITY/agency/CRTM> foaf:page ?agencyPage .
	<http://linkedgtfs.com/CITY/agency/CRTM> foaf:name ?agencyName .
	<http://linkedgtfs.com/CITY/agency/CRTM> foaf:phone ?agencyPhone .

	OPTIONAL {
		?route a gtfs:Route .
		?route gtfs:agency <http://linkedgtfs.com/CITY/agency/CRTM> .
	}
}
